{
	"$schema": "./umbraco/config/appsettings-schema.json",
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"Microsoft.Hosting.Lifetime": "Information",
				"System": "Warning"
			}
		}
	},
	"ConnectionStrings": {
		"umbracoDbDSN": "Data Source=(localdb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\Umbraco.mdf;Integrated Security=True"
	},
	"Umbraco": {
		"CMS": {
			"Hosting": {
				"Debug": false
			},
			"Global": {
				"Id": "6821879e-9677-4c7b-9b52-bb8447265466"
			},
			"ModelsBuilder": {
				"ModelsMode": "SourceCodeAuto"
			}
		},
		"Imaging": {
			"ImageFileTypes": [ "jpeg", "jpg", "gif", "bmp", "png", "tiff", "tif", "webp" ]
		}
	}
}