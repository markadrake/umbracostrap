{
	"name": "umbracostrap.ui",
	"version": "0.0.0",
	"description": "Umbracostrap.UI",
	"author": {
		"name": ""
	},
	"scripts": {
		"build": "parcel build ./src/index.html --no-content-hash --no-optimize",
		"debug": "parcel build ./src/index.html --no-content-hash --no-optimize --log-level verbose",
		"css-lint": "stylelint \"**/*.scss\"",
		"prebuild": "rimraf dist",
		"start": "parcel serve ./src/index.html",
		"test": "npm-run-all css-lint",
		"watch": "parcel watch ./src/index.html --no-content-hash",
		"build-dev": "webpack --mode development",
		"build-prod": "webpack --mode production"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.2",
		"bootstrap": "^5.1.3",
		"bootstrap-icons": "^1.8.0"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^4.1.3",
		"@parcel/transformer-sass": "^2.2.1",
		"autoprefixer": "^10.4.2",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^10.2.4",
		"css-loader": "^6.6.0",
		"mini-css-extract-plugin": "^2.5.3",
		"node-sass": "^7.0.1",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.2.1",
		"postcss": "^8.4.6",
		"postcss-cli": "^9.1.0",
		"postcss-custom-properties": "^12.1.4",
		"postcss-import": "^14.0.2",
		"postcss-url": "^10.1.3",
		"rimraf": "^3.0.2",
		"sass": "^1.49.7",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.3.0",
		"stylelint-config-twbs-bootstrap": "^3.0.1",
		"webpack": "^5.68.0",
		"webpack-cli": "^4.9.2"
	}
